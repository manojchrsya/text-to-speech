{
    "env": {
      "commonjs": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
      "Story": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "global-require": "off",
      "consistent-return": "off",
      "no-console": "error"
    }
}
